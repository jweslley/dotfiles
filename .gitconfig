[github]
  user = jweslley
  token = 
[user]
  name = Jonhnny Weslley
  email = jw@jonhnnyweslley.net
[color]
  ui = auto
  sh = auto
[alias]
  s = status
  lc = log ORIG_HEAD.. --stat --no-merges
  l = log --graph --pretty=format:'%C(yellow)%h%Creset -%C(bold red)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  amend = commit --amend
  aliases = !git config --get-regexp 'alias.*' | colrm 1 6 | sed 's/[ ]/ = /' | sort | grep -E '([a-z\\-]+) =' --color
  au = !git add $(git ls-files -o --exclude-standard)
  ad = !git rm $(git ls-files -d)
  edit-unmerged = "!f() { git ls-files --unmerged | cut -f2 | sort -u ; }; vim `f`"
  add-unmerged = "!f() { git ls-files --unmerged | cut -f2 | sort -u ; }; git add `f`"
[color "sh"]
  branch = yellow
  workdir = blue
  dirty = red
